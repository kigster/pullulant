# CAPS LINE
regexp=^[A-Z\s%]*([A-Z]{3})[A-Z\s%]*$
colours=underline
=======
# /full/path
regexp=(\s|^)/[-\w\d.]+/[-\w\d./]+
colours=white
=======
# Capd Line
regexp=^([A-Z][-a-z0-9]+(\s+|$)){3,}$
colours=underline
=======
# VMSTAT
regexp=(\s|^)\d+([.,]\d+)?(?=[\s,]|$)
colours=bold green
=======
# n.n.n
regexp=(\s|^)\d+\.\d+\.\d+(?=[\s,]|$)
colours=bold cyan
=======
# text:
regexp=^([-a-z0-9]+):\s
colours=unchanged,yellow
=======
# -options
regexp=(?<=\s)-[\w\d]+(?=\s|$)
colours=yellow
=======
# --long-option=
regexp=(?<=\s)--[-\w\d]+[\w\d](?==|\s|$)(=?)
colours=cyan,white
=======
# [text]
regexp=\[[-\w\d:/]+\]
colours=cyan
=======
# root user
regexp=root|wheel(?=\s|$)
colours=red
=======
# pts
regexp=(\s|^)pts/\d+(?=[^\w\d]|$)
colours=bold yellow
=======
# tty
regexp=(\s|^)tty\d+(?=[^\w\d]|$)
colours=bold cyan
=======
# time
regexp=(\d*:)?\d*:\d{2}
colours=bold white
=======
# time 00:00:00
regexp=00:00:00
colours=bold black
======
# time 0:00
regexp=\s0:00\s
colours=bold black
======
# ?
regexp=\s\?\s
colours=red
=======
# Process state
regexp=\s([DRSTEXZ])([NLsl+<]*)\s
colours=cyan, bold blue
