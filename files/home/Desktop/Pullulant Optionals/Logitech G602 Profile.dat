<?xml version="1.0" encoding="utf-8"?>
<obprofiles xmlns="http://www.logitech.com/Cassandra/2013.1/OnboardProfile">
  <profile name="">
    <device model="Logitech.Gaming.Mouse.G602" reportrate="125" anglesnapping="0" buttoncount="11" powermode="1">
      <dpitable shift="500" currentindex="4" defaultindex="4">
        <dpi x="500" y="500"/>
        <dpi x="1000" y="1000"/>
        <dpi x="1500" y="1500"/>
        <dpi x="2000" y="2000"/>
        <dpi x="2500" y="2500"/>
      </dpitable>
    </device>
    <buttons>
      <button number="1" name="Left Click">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="leftclick"/>
        </mousefunction>
      </button>
      <button number="2" name="Right Click">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="rightclick"/>
        </mousefunction>
      </button>
      <button number="3" name="Middle Click">
        <mousefunction xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/MouseFunction">
          <do task="middleclick"/>
        </mousefunction>
      </button>
      <button number="4" name="Left Cmd + RBRACKET">
        <keystroke xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/Keystroke">
          <key value="RBRACKET"/>
          <modifier value="LGUI"/>
        </keystroke>
      </button>
      <button number="5" name="Left Cmd + LBRACKET">
        <keystroke xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/Keystroke">
          <key value="LBRACKET"/>
          <modifier value="LGUI"/>
        </keystroke>
      </button>
      <button number="6" name="Shift + Left Cmd + 5">
        <keystroke xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/Keystroke">
          <key value="5"/>
          <modifier value="LSHIFT"/>
          <modifier value="LGUI"/>
        </keystroke>
      </button>
      <button number="7" name="Ctrl + Alt + Left Cmd + Left">
        <keystroke xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/Keystroke">
          <key value="LEFT"/>
          <modifier value="LCTRL"/>
          <modifier value="LALT"/>
          <modifier value="LGUI"/>
        </keystroke>
      </button>
      <button number="8" name="Ctrl + Alt + Left Cmd + Right">
        <keystroke xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/Keystroke">
          <key value="RIGHT"/>
          <modifier value="LCTRL"/>
          <modifier value="LALT"/>
          <modifier value="LGUI"/>
        </keystroke>
      </button>
      <button number="9" name="Ctrl + Up">
        <keystroke xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/Keystroke">
          <key value="UP"/>
          <modifier value="LCTRL"/>
        </keystroke>
      </button>
      <button number="10" name="Ctrl + Alt + Left Cmd + =">
        <keystroke xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/Keystroke">
          <key value="EQUAL"/>
          <modifier value="LCTRL"/>
          <modifier value="LALT"/>
          <modifier value="LGUI"/>
        </keystroke>
      </button>
      <button number="11" name="Ctrl + Alt + Left Cmd + -">
        <keystroke xmlns="http://www.logitech.com/Cassandra/2010.1/Macros/Keystroke">
          <key value="MINUS"/>
          <modifier value="LCTRL"/>
          <modifier value="LALT"/>
          <modifier value="LGUI"/>
        </keystroke>
      </button>
    </buttons>
  </profile>
</obprofiles>
